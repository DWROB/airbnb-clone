<%= simple_form_for [apartment, booking] do |f| %>
  <%= f.input :start_date, as: :string, input_html: { data: { bookings_target: "startDate" } }  %>
  <%= f.input :end_date, as: :string, input_html: { data: { bookings_target: "endDate", action: "change->bookings#showDetails" } } %>
  <%= f.submit %>
<% end %>
